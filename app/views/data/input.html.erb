<div class="container">
	<div class="row row-centered">
		<div class="col-xs-6 col-centered">
<h1>What foods that what you want?</h1>
<%= check_box_tag 'id-food', 'indonesian-food'  %>
<%= label_tag 'id-food', 'Indonesian food' %>
<%= check_box_tag 'in-food', 'international-food'  %>
<%= label_tag 'in-food', 'International food' %>

	<div id="open-id">
		<%= render "idfood"  %>
	</div>
		<div id="open-in">
		<h1>Searching for International foods</h1>
		<h2>Here is suggestion International top dishes</h2>
		<ul>
			<li>Hamburger</li>
			<li>Sandwich</li>
			<li>Pizza</li>
			<li>Fried chicken</li>
			<li>Macaroni</li>
		</ul>
	</div>
	<div id="form_search">
		<%= form_tag data_search_path, method:'get', remote: true do   %>
		<%= text_field_tag :search, '', size:20, :autocomplete => :off, :class => "searchbox"  %>
		<%= submit_tag 'search', :id => 'search_key', :style => "display: none;" %>
		<% end %>
	</div>

<div id="food"></div>
</div>
</div>
</div>